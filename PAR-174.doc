src_port={$src_port}
protocol=udp

#Nov  6 04:32:54 ahtapot conpot 2018-11-07 10:32:24,114 New TFTP client has connected. Connection from 10.0.3.254:40946.

#TFTP 
[0004 - conpot_tftp]
event_type=event
regexp='\w+\s+\d+\s\d+:\d+:\d+\s(?P&lt;device&gt;.*?)\s(?P&lt;sensor&gt;.*?)\s(?P&lt;date&gt;\d+-\d+-\d+ \d+:\d+:\d+),\d+\s\w+\s(?P&lt;app&gt;TFTP)\s\w+\s\w+\s\w+.\s\w+\s\w+\s(?P&lt;src_ip&gt;\d{1,3}.{1,3}.{1,3}.{1,3}):(?P&lt;src_port&gt;\d+).'
date={$date}
device={resolve($device)}
plugin_sid={translate($app)}
