protocol=udp


#Nov  6 04:30:26 ahtapot conpot 2018-11-07 10:29:56,369 handle server TID [  100/140140425411656] starting on ('10.0.3.254', 35710)

#TID 
[0005 - conpot_TID]
event_type=event
regexp='\w+\s+\d+\s\d+:\d+:\d+\s(?P&lt;device&gt;.*?)\s(?P&lt;sensor&gt;.*?)\s(?P&lt;date&gt;\d+-\d+-\d+ \d+:\d+:\d+),\d+\s\w+\s\w+\s(?P&lt;app&gt;TID)\s\[\s+\d+/\d+\]\s\w+\s\w+\s\(\'(?P&lt;src_ip&gt;\d{1,3}.{1,3}.{1,3}.{1,3})\',\s(?P&lt;src_port&gt;\d+)\)'
date={$date}
device={resolve($device)}
plugin_sid={translate($app)}
src_ip={$src_ip}
