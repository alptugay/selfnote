protocol=tcp

#Nov  6 04:31:59 ahtapot conpot 2018-11-07 10:31:29,734 New Bacnet connection from 10.0.3.254:51491. (472a06d6-5aa2-4c77-b696-ca5c5b20967b)

#Bacnet
[0003 - conpot_bacnet]
event_type=event
regexp='\w+\s+\d+\s\d+:\d+:\d+\s(?P&lt;device&gt;.*?)\s(?P&lt;sensor&gt;.*?)\s(?P&lt;date&gt;\d+-\d+-\d+ \d+:\d+:\d+),\d+\s\w+\s(?P&lt;app&gt;Bacnet)\s\w+\s\w+\s(?P&lt;src_ip&gt;\d{1,3}.{1,3}.{1,3}.{1,3}):(?P&lt;src_port&gt;\d+).'
date={$date}
device={resolve($device)}
plugin_sid={translate($app)}
